{
  "name": "behindthep/diff-comp",
  "bin": [
    "bin/gendiff"
  ],
  "description": "Difference Comparator.",
  "type": "project",
  "authors": [
    {
      "name": "Alex Kolvakh",
      "email": "alex.kolvah@mail.ru"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Diff\\Comparator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Diff\\Comparator\\Tests\\": "tests"
    }
  },
  "require": {
    "phpstan/phpstan": "^2.1",
    "symfony/yaml": "^7.2",
    "docopt/docopt": "^1.0",
    "lstrojny/functional-php": "^1.17"
  },
  "require-dev": {
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/extension-installer": "^1.3",
    "symfony/var-dumper": "^7.2",
    "phpunit/phpunit": "^11.5",
    "squizlabs/php_codesniffer": "*"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "license": "MIT"
}
